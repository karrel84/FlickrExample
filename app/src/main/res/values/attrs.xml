<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!-- ExpandableTextView -->
    <declare-styleable name="ExpandTextView">
        <attr name="android:textSize" format="dimension" />
        <attr name="android:textColor" format="color" />
        <attr name="maxCollapsedLines" format="integer" />
        <attr name="expandDrawable" format="reference" />
        <attr name="collapseDrawable" format="reference" />
    </declare-styleable>
</resources>